    "homepage": "https://gitlab.com/pfurak/php-xlsxwriter",
    "license": "MIT",
    "readme": "README.md",
    "authors": [
        {
            "name": "Patrik Zsolt Furak",
            "email": "furak.patrik@hotmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "ext-zip": "*"
    },
    "autoload": {
        "psr-4": {
